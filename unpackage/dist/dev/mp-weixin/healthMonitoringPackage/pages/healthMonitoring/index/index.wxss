page {
  width: 100%;
  height: 100%;
}
.content-box {
  display: flex;
  min-height: 100vh;
  height: 100%;
  width: 100%;
  flex-direction: column;
  padding-bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.content-box ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.content-box .nav {
  width: 100%;
  background: #fff;
}
.content-box .nav .header_content .header_left_box > view {
  width: 100px;
  padding-left: 16px;
  box-sizing: border-box;
  font-size: 16px;
  color: #101010;
}
.content-box .content-area {
  padding: 10px;
  background: #f5f5f5;
  box-sizing: border-box;
  flex: 1;
  overflow: auto;
}
.content-box .content-area .show-home-content .show-title {
  display: flex;
  flex-direction: column;
}
.content-box .content-area .show-home-content .show-title > text:first-child {
  font-size: 18px;
  color: #101010;
}
.content-box .content-area .show-home-content .show-title > text:last-child {
  font-size: 14px;
  color: #101010;
  margin: 10px 0;
}
.content-box .content-area .show-home-content .show-list-wrapper {
  min-height: 200px;
  position: relative;
}
.content-box .content-area .show-home-content .show-list-wrapper  .u-empty {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.content-box .content-area .show-home-content .show-list-wrapper .show-list {
  display: flex;
  padding: 0 6px;
  align-items: center;
  justify-content: space-between;
  background: #fff;
  margin-bottom: 8px;
  width: 100%;
  height: 46px;
}
.content-box .content-area .show-home-content .show-list-wrapper .show-list:last-child {
  margin-bottom: 0 !important;
}
.content-box .content-area .show-home-content .show-list-wrapper .show-list .list-left > image {
  width: 24px;
  height: 24px;
  vertical-align: middle;
  margin-right: 10px;
}
.content-box .content-area .show-home-content .show-list-wrapper .show-list .list-left .scene {
  color: #101010;
}
.content-box .content-area .show-home-content .show-list-wrapper .show-list .list-left .scene-box {
  color: #898C8C;
}
.content-box .content-area .show-home-content .show-list-wrapper .show-list .list-left > text {
  font-size: 16px;
  vertical-align: middle;
}
.content-box .content-area .show-home-content .show-list-wrapper .show-list .list-right {
  display: flex;
  justify-content: flex-end;
}
.content-box .content-area .show-home-content .show-list-wrapper .show-list .list-right > view {
  margin-right: 24px;
  position: relative;
}
.content-box .content-area .show-home-content .show-list-wrapper .show-list .list-right > view image {
  width: 20px;
  height: 20px;
  vertical-align: middle;
}
.content-box .content-area .show-home-content .show-list-wrapper .show-list .list-right .delete-box::after {
  content: "";
  position: absolute;
  top: 2px;
  right: -13px;
  width: 1px;
  height: 22px;
  background: #BBBBBB;
}
.content-box .content-area .show-home-content .show-list-wrapper .show-list .list-right .copy-box::after {
  content: "";
  position: absolute;
  top: 2px;
  right: -13px;
  width: 1px;
  height: 22px;
  background: #BBBBBB;
}
.content-box .content-area .show-home-content .show-list-wrapper .show-list .list-right .move-box {
  margin-right: 0 !important;
}
.content-box .content-area .no-show-home-content .show-title {
  display: flex;
  flex-direction: column;
}
.content-box .content-area .no-show-home-content .show-title > text:first-child {
  font-size: 18px;
  color: #101010;
}
.content-box .bottom-area {
  background: #f5f5f5;
  height: 120px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content-box .bottom-area .btn-area {
  border-radius: 26px;
  width: 266px;
  height: 48px;
  background: #11D183;
  box-shadow: 0px 2px 6px 0 rgba(92, 133, 136, 0.29);
  font-size: 16px;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
}

