page {
  width: 100%;
  height: 100%;
}
.content-box {
  display: flex;
  min-height: 100vh;
  height: 100%;
  width: 100%;
  flex-direction: column;
  padding-bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.content-box ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.content-box .uni-ec-canvas {
  width: 100%;
  height: 100px;
  display: block;
}
.content-box .top-area {
  height: 50px;
  display: flex;
  align-items: center;
  padding: 10px;
  box-sizing: border-box;
  width: 100%;
  background: #fff;
  margin-top: 44px;
}
.content-box .top-area > view:nth-child(1) {
  font-size: 20px;
  color: #11D183;
}
.content-box .top-area > view:nth-child(2) {
  width: 140px;
  height: 40px;
  margin-top: 2px;
}
.content-box .top-area > view:nth-child(2)  .show-box {
  border: none !important;
  font-size: 16px;
  height: 40px;
  color: #0E2442 !important;
  background: transparent !important;
}
.content-box .top-area > view:nth-child(2)  .show-box .input {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  display: inline-block;
  line-height: 40px;
  height: 40px;
  padding-right: 4px;
  box-sizing: border-box;
  text-align: center !important;
}
.content-box .top-area > view:nth-child(2)  .show-box .list {
  color: #101010;
}
.content-box .top-area > view:nth-child(2)  .show-box .list-container {
  width: 150px !important;
  top: -10px;
}
.content-box .top-area > view:nth-child(2)  .show-box .list-container .popper__arrow {
  display: none !important;
}
.content-box .top-area > view:nth-child(2)  .show-box .placeholder {
  color: #0E2442 !important;
}
.content-box .top-area > view:nth-child(2)  .show-box .right-arrow {
  color: #0E2442 !important;
}
.content-box .banner-area-box {
  padding: 0 10px 10px 10px;
  min-height: 135px;
  box-sizing: border-box;
  background: #fff;
}
.content-box .center-area {
  flex: 1;
  overflow: auto;
  background: #f5f5f5;
  position: relative;
}
.content-box .center-area .charts-box {
  width: 100%;
  height: 150px;
}
.content-box .center-area  .u-empty {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.content-box .center-area .bind-sleep-device-area {
  margin-top: 2px;
  padding: 4px 10px;
  box-sizing: border-box;
  background: #fff;
  height: 275px;
  border-radius: 10px;
  display: flex;
  flex-direction: column;
}
.content-box .center-area .bind-sleep-device-area > view:nth-child(1) {
  font-size: 14px;
  color: #101010;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.content-box .center-area .bind-sleep-device-area > view:nth-child(2) {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.content-box .center-area .bind-sleep-device-area > view:nth-child(2) > view {
  width: 100%;
}
.content-box .center-area .bind-sleep-device-area > view:nth-child(2) > view:nth-child(1) {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 14px;
  color: #101010;
  margin-bottom: 20px;
}
.content-box .center-area .bind-sleep-device-area > view:nth-child(2) > view:nth-child(1) > text {
  text-align: center;
  display: inline-block;
  width: 100%;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.content-box .center-area .bind-sleep-device-area > view:nth-child(2) > view:nth-child(2) {
  justify-content: center;
  font-size: 14px;
  display: flex;
  align-items: center;
}
.content-box .center-area .bind-sleep-device-area > view:nth-child(2) > view:nth-child(2) > text {
  color: #101010;
}
.content-box .center-area .bind-sleep-device-area > view:nth-child(2) > view:nth-child(2) > text:nth-child(2) {
  display: inline-block;
  width: 89px;
  height: 26px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #11D183;
  border-radius: 4px;
  color: #fff !important;
  margin: 0 6px;
}
.content-box .center-area .bind-toileting-device-area {
  height: 159px !important;
  margin-top: 4px;
}
.content-box .center-area .tumble-toileting-device-area {
  height: 132px !important;
  margin-top: 4px;
}
.content-box .center-area .leave-home-device-area {
  height: 159px !important;
  margin-top: 4px;
  margin-bottom: 4px;
}
.content-box .center-area .sleep-area-data {
  height: 397px;
  background: #fff;
  border-radius: 10px;
  margin-top: 6px;
  padding: 4px 10px;
  box-sizing: border-box;
}
.content-box .center-area .sleep-area-data .sleep-data-title {
  height: 32px;
  display: flex;
  align-items: center;
}
.content-box .center-area .sleep-area-data .sleep-data-title > text {
  margin-right: 10px;
  margin-top: 2px;
  font-size: 12px;
  color: #BBBBBB;
}
.content-box .center-area .sleep-area-data .sleep-data-title > text:first-child {
  font-size: 14px;
  color: #101010;
  margin-top: 0 !important;
}
.content-box .center-area .sleep-area-data .sleep-data-title .room-name {
  position: relative;
}
.content-box .center-area .sleep-area-data .sleep-data-title .room-name::after {
  content: "";
  position: absolute;
  top: 1px;
  right: -6px;
  width: 1px;
  height: 14px;
  background: #BBBBBB;
}
.content-box .center-area .sleep-area-data .heart-rate-box {
  padding: 6px;
  box-sizing: border-box;
  border: 1px dashed #BBBBBB;
  border-radius: 10px;
  box-shadow: 0px 2px 6px 0 rgba(0, 0, 9, 0.1);
  height: 111px;
  margin-bottom: 8px;
}
.content-box .center-area .sleep-area-data .heart-rate-box .heart-rate-title {
  display: flex;
  justify-content: space-between;
}
.content-box .center-area .sleep-area-data .heart-rate-box .heart-rate-title .heart-rate-title-left > image {
  width: 19px;
  height: 19px;
  vertical-align: middle;
  margin-right: 6px;
}
.content-box .center-area .sleep-area-data .heart-rate-box .heart-rate-title .heart-rate-title-left > text {
  font-size: 14px;
  color: #101010;
  vertical-align: middle;
  margin-right: 6px;
}
.content-box .center-area .sleep-area-data .heart-rate-box .heart-rate-title .heart-rate-title-left > text:last-child {
  font-size: 12px;
}
.content-box .center-area .sleep-area-data .heart-rate-box .heart-rate-title .heart-rate-title-right {
  font-size: 12px;
  color: #007AFF;
}
.content-box .center-area .toilet-area-data {
  height: 159px;
  margin-top: 8px;
}
.content-box .center-area .tumble-area-data {
  height: 132px;
  margin-top: 8px;
}
.content-box .center-area .tumble-area-data .heart-rate-box {
  height: 87px !important;
}
.content-box .center-area .leave-home-area-data {
  height: 159px;
  margin-top: 8px;
  margin-bottom: 8px;
}
.content-box .bottom-area {
  margin: 0 auto;
  width: 353px;
  border-radius: 5px;
  height: 38px;
  text-align: center;
  line-height: 38px;
  border: 1px solid #11D183;
  color: #11D183;
  font-size: 14px;
}

