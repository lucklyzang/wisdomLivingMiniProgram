page {
  width: 100%;
  height: 100%;
}
.content-box {
  display: flex;
  min-height: 100vh;
  height: 100%;
  width: 100%;
  flex-direction: column;
}
.content-box .top-area-box {
  position: relative;
  width: 100%;
  height: 230px;
}
.content-box .top-area-box > image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 230px;
}
.content-box .top-area-box .operation-area {
  width: 90%;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  z-index: 1;
  margin-top: 44px;
  position: relative;
}
.content-box .top-area-box .operation-area .operation-top-area {
  width: 100%;
  display: flex;
  align-items: center;
}
.content-box .top-area-box .operation-area .operation-top-area .image-area image {
  width: 29px;
  height: 29px;
}
.content-box .top-area-box .operation-area .operation-top-area .image-area image:first-child {
  margin-right: 20px;
}
.content-box .top-area-box .operation-area .operation-top-area .dropdown-area {
  width: 100px;
  -webkit-transform: translateX(50%);
          transform: translateX(50%);
}
.content-box .top-area-box .operation-area .operation-top-area .dropdown-area  .show-box {
  border: none !important;
  color: #fff !important;
  background: transparent !important;
}
.content-box .top-area-box .operation-area .operation-top-area .dropdown-area  .show-box .input {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  display: inline-block;
  line-height: 42px;
  padding-right: 4px;
  box-sizing: border-box;
  text-align: center !important;
}
.content-box .top-area-box .operation-area .operation-top-area .dropdown-area  .show-box .list {
  color: #101010;
}
.content-box .top-area-box .operation-area .operation-top-area .dropdown-area  .show-box .list-container {
  width: 150px !important;
  top: -10px;
}
.content-box .top-area-box .operation-area .operation-top-area .dropdown-area  .show-box .list-container .popper__arrow {
  display: none !important;
}
.content-box .top-area-box .operation-area .operation-top-area .dropdown-area  .show-box .placeholder {
  color: #fff !important;
}
.content-box .top-area-box .operation-area .operation-top-area .dropdown-area  .show-box .right-arrow {
  color: #fff !important;
}
.content-box .top-area-box .cut-title-area {
  width: 30%;
  margin: 0 auto;
  font-size: 14px;
  color: #898C8C;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: absolute;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  bottom: 40px;
}
.content-box .top-area-box .cut-title-area .activeTabStyle {
  font-size: 16px !important;
  color: #fff !important;
}
.content-box .center-content-area {
  width: 98%;
  margin: 0 auto;
  flex: 1;
  overflow: auto;
}
.content-box .center-content-area .device-content-area {
  width: 98%;
  padding: 10px;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.content-box .center-content-area .device-content-area .device-list {
  width: 47%;
  height: 102px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-bottom: 10px;
  border-radius: 10px;
  box-shadow: 0px 2px 6px 0 rgba(0, 0, 9, 0.1);
  padding: 6px 10px;
  box-sizing: border-box;
}
.content-box .center-content-area .device-content-area .device-list .device-top {
  display: flex;
  justify-content: space-between;
}
.content-box .center-content-area .device-content-area .device-list .device-top .device-top-left {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.content-box .center-content-area .device-content-area .device-list .device-top .device-top-left > image {
  width: 34px;
  height: 42px;
}
.content-box .center-content-area .device-content-area .device-list .device-top .device-top-left > text {
  font-size: 12px;
  color: #BBBBBB;
}
.content-box .center-content-area .device-content-area .device-list .device-top .device-top-right > text {
  font-size: 14px;
  color: #BBBBBB;
}
.content-box .center-content-area .device-content-area .device-list .device-top .device-top-right .onLineStyle {
  coloe: #0079FF !important;
}
.content-box .center-content-area .device-content-area .device-list .device-bottom {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.content-box .center-content-area .device-content-area .device-list .device-bottom text {
  font-size: 14px;
  color: #101010;
}
.content-box .center-content-area .room-content-area {
  width: 98%;
  padding: 10px;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  margin-top: -40px;
  position: absolute;
  flex-grow: 0;
}
.content-box .center-content-area .room-content-area .room-list {
  display: flex;
  height: 110px;
  justify-content: space-between;
  margin-bottom: 10px;
  border-radius: 10px;
  box-shadow: 0px 2px 6px 0 rgba(0, 0, 9, 0.1);
  padding: 10px;
  box-sizing: border-box;
  background: #fff;
}
.content-box .center-content-area .room-content-area .room-list .room-left {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.content-box .center-content-area .room-content-area .room-list .room-left .room-left-top {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.content-box .center-content-area .room-content-area .room-list .room-left .room-left-top > text:first-child {
  font-size: 14px;
  color: #101010;
  margin-bottom: 4px;
}
.content-box .center-content-area .room-content-area .room-list .room-left .room-left-top > text:last-child {
  font-size: 12px;
  color: #BBBBBB;
}
.content-box .center-content-area .room-content-area .room-list .room-left .room-left-bottom > image {
  width: 34px;
  height: 42px;
  margin-right: 4px;
}
.content-box .center-content-area .room-content-area .room-list .room-right {
  display: flex;
  align-items: center;
}

