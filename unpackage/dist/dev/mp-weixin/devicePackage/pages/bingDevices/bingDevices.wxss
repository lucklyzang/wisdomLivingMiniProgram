page {
  width: 100%;
  height: 100%;
}
.content-box {
  display: flex;
  min-height: 100vh;
  height: 100%;
  width: 100%;
  flex-direction: column;
  padding-bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.content-box ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.content-box .nav {
  width: 100%;
  background: #fff;
}
.content-box .content-area {
  flex: 1;
  padding: 16px;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.content-box .content-area .content-top {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 90px;
}
.content-box .content-area .content-top > text {
  font-size: 16px;
  font-weight: bold;
  color: #101010;
}
.content-box .content-area .content-top > text:first-child {
  margin-bottom: 10px;
}
.content-box .content-area .content-bottom {
  flex: 1;
  overflow: auto;
  margin-top: 40px;
}
.content-box .content-area .content-bottom .devices-list {
  width: 175px;
  height: 102px;
  border-radius: 10px;
  box-shadow: 0px 1px 6px 0 rgba(0, 0, 0, 0.1);
  padding: 6px 16px;
  box-sizing: border-box;
  margin-bottom: 16px;
}
.content-box .content-area .content-bottom .devices-list .list-top {
  display: flex;
  justify-content: space-between;
}
.content-box .content-area .content-bottom .devices-list .list-top .list-top-left > image {
  width: 38px;
  height: 39px;
}
.content-box .content-area .content-bottom .devices-list .list-top .list-top-right > text {
  font-size: 14px;
  color: #0079FF;
}
.content-box .content-area .content-bottom .devices-list .list-bottom {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.content-box .content-area .content-bottom .devices-list .list-bottom > text {
  color: #101010;
}
.content-box .content-area .content-bottom .devices-list .list-bottom > text:first-child {
  font-size: 18px;
  margin-bottom: 4px;
}
.content-box .content-area .content-bottom .devices-list .list-bottom > text:last-child {
  font-size: 16px;
}
.content-box .content-no-query-device-area {
  padding: 16px;
  box-sizing: border-box;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.content-box .content-no-query-device-area .content-top {
  height: 55vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-end;
}
.content-box .content-no-query-device-area .content-top .no-query-device > image {
  width: 94px;
  height: 94px;
}
.content-box .content-no-query-device-area .content-top .no-query-device-info {
  margin-top: 40px;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.content-box .content-no-query-device-area .content-top .no-query-device-info > text {
  font-size: 18px;
  color: #101010;
}
.content-box .content-no-query-device-area .content-top .no-query-device-info > text:nth-child(2) {
  margin: 8px 0;
}
.content-box .content-no-query-device-area .content-top .no-query-device-info > text:nth-child(3) {
  font-size: 14px;
}
.content-box .content-no-query-device-area .content-bottom {
  height: 45vh;
  display: flex;
  align-items: center;
  justify-content: center;
}
.content-box .content-no-query-device-area .content-bottom > text {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 319px;
  height: 55px;
  border: 1px solid #11D183;
  border-radius: 28px;
  color: #11D183;
  font-size: 18px;
}

