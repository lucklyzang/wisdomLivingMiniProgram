page {
  width: 100%;
  height: 100%;
}
.content-box {
  display: flex;
  min-height: 100vh;
  height: 100%;
  width: 100%;
  flex-direction: column;
  padding-bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.content-box ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content {
  padding: 20px 30px;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .wifi-title {
  font-size: 14px;
  color: rgba(14, 36, 66, 0.56);
  margin-bottom: 10px;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .wifi-list-wrapper {
  flex: 1;
  overflow: auto;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .wifi-list-wrapper .wifi-list {
  display: flex;
  height: 50px;
  justify-content: space-between;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .wifi-list-wrapper .wifi-list .wifi-list-left {
  display: flex;
  flex-direction: column;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .wifi-list-wrapper .wifi-list .wifi-list-left > text {
  font-size: 18px;
  color: #101010;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .wifi-list-wrapper .wifi-list .wifi-list-left .current-connect {
  font-size: 14px;
  color: rgba(14, 36, 66, 0.56);
}
.content-box .nav {
  width: 100%;
  background: #fff;
}
.content-box .content-area {
  width: 100%;
  padding: 16px;
  box-sizing: border-box;
  flex: 1;
  overflow: auto;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.content-box .content-area .content-top-area {
  width: 100%;
  flex: 1;
  display: flex;
  flex-direction: column;
}
.content-box .content-area .content-top-area .top-title {
  display: flex;
  flex-direction: column;
  margin-bottom: 40px;
}
.content-box .content-area .content-top-area .top-title > text:first-child {
  font-size: 22px;
  color: #101010;
}
.content-box .content-area .content-top-area .top-title > text:last-child {
  margin-top: 4px;
  font-size: 14px;
  color: #0E2442;
}
.content-box .content-area .content-top-area .form-area {
  flex: 1;
  overflow: auto;
}
.content-box .content-area .content-top-area .form-area  .u-form .u-form-item {
  background: rgba(229, 229, 229, 0.41);
  border-radius: 13px;
  padding: 2px 8px !important;
  margin-bottom: 30px;
}
.content-box .content-area .content-top-area .form-area  .u-form .u-form-item .u-form-item--right__content__icon {
  font-size: 14px;
  color: #34B9C0;
}
.content-box .content-area .content-top-area .form-area  .u-form .u-form-item .uni-input-placeholder {
  color: #C1C2C5 !important;
  font-size: 14px !important;
}
.content-box .content-area .operation-area {
  height: 80px;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content-box .content-area .operation-area > text {
  font-size: 14px;
  display: inline-block;
  border-radius: 26px;
  width: 60%;
  height: 40px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #11D183;
  color: #fff;
}

