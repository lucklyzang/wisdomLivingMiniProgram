page {
  width: 100%;
  height: 100%;
}
.content-box {
  display: flex;
  min-height: 100vh;
  height: 100%;
  width: 100%;
  flex-direction: column;
  padding-bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.content-box ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.content-box .content-area {
  padding: 0 16px 16px 16px;
  box-sizing: border-box;
  flex: 1;
  display: flex;
  flex-direction: column;
}
.content-box .content-area .content-top-title {
  height: 44px;
  display: flex;
  align-items: center;
  font-size: 22px;
  color: #101010;
}
.content-box .content-area .content-center {
  flex: 1;
  display: flex;
  flex-direction: column;
}
.content-box .content-area .content-center .content-center-top {
  flex: 1;
  display: flex;
  flex-direction: column;
}
.content-box .content-area .content-center .content-center-top > image {
  width: 83px;
  height: 100px;
  margin-bottom: 10px;
  margin-top: 4px;
}
.content-box .content-area .content-center .content-center-top .room-list-wrapper {
  flex: 1;
  overflow: auto;
  display: flex;
  align-content: flex-start;
  flex-wrap: wrap;
}
.content-box .content-area .content-center .content-center-top .room-list-wrapper .room-list {
  height: 44px;
  padding: 0 14px;
  text-align: center;
  line-height: 44px;
  background: rgba(229, 229, 229, 0.41);
  border-radius: 10px;
  color: #101010;
  font-size: 16px;
  margin-right: 20px;
  margin-bottom: 20px;
}
.content-box .content-area .content-center .content-center-top .room-list-wrapper .roomListStyle {
  background: #11D183;
  color: #fff;
}
.content-box .content-area .content-center .content-center-top .create-new-room {
  margin-top: 10px;
  width: 130px;
  height: 44px;
  background: rgba(229, 229, 229, 0.41);
  border-radius: 10px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.content-box .content-area .content-center .content-center-top .create-new-room  .u-icon {
  margin-top: 2px;
}
.content-box .content-area .content-center .content-center-top .create-new-room > text {
  margin-left: 4px;
  color: #34b9c0;
  font-size: 16px;
}
.content-box .content-area .content-center .content-center-top .page-area {
  display: flex;
  justify-content: center;
  align-items: center;
}
.content-box .content-area .content-center .content-center-top .page-area > text {
  font-size: 14px;
  color: rgba(14, 36, 66, 0.56);
}
.content-box .content-area .bottom-btn {
  height: 70px;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content-box .content-area .bottom-btn > text {
  font-size: 14px;
  display: inline-block;
  border-radius: 26px;
  width: 60%;
  height: 40px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #11D183;
  color: #fff;
}

