page {
  width: 100%;
  height: 100%;
}
.content-box {
  display: flex;
  min-height: 100vh;
  height: 100%;
  width: 100%;
  flex-direction: column;
  padding-bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.content-box ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content {
  padding: 20px 40px;
  box-sizing: border-box;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .top-title {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 10px;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .top-title .top-title-one > text {
  font-size: 18px;
  color: #101010;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .top-title .top-title-two {
  margin-top: 10px;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .top-title .top-title-two > text {
  font-size: 14px;
  color: rgba(14, 36, 66, 0.56);
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .top-title .top-title-two > text:nth-child(even) {
  color: blue;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .bottom-btn {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .bottom-btn > text {
  display: inline-block;
  width: 40%;
  height: 40px;
  text-align: center;
  line-height: 40px;
  border-radius: 20px;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .bottom-btn > text:first-child {
  background: rgba(136, 136, 136, 0.63);
  color: #101010;
}
.content-box .wifi-list-box  .u-drawer .u-drawer-content .bottom-btn > text:last-child {
  background: #11d183;
  color: #fff;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content {
  padding-bottom: 40px;
  box-sizing: border-box;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .top-title-name-area {
  height: 60px;
  width: 100%;
  font-size: 18px;
  color: #A7A7A3;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  border: none;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .top-title-name-area:after {
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  background-color: #BBBBBB;
  -webkit-transform: scaleY(0.5);
          transform: scaleY(0.5);
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .accept-alarm-method-list-wrapper {
  padding-top: 10px;
  box-sizing: border-box;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .accept-alarm-method-list-wrapper .accept-alarm-method-list {
  text-align: center;
  margin-bottom: 16px;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .accept-alarm-method-list-wrapper .accept-alarm-method-list > text {
  font-size: 20px;
  color: #101010;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .accept-alarm-method-list-wrapper .accept-alarm-method-list .textMethodStyle {
  color: #11D183 !important;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .cancel-btn {
  width: 100%;
  display: flex;
  height: 80px;
  align-items: center;
  justify-content: center;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .cancel-btn > text {
  width: 80%;
  display: inline-block;
  height: 48px;
  font-size: 18px;
  color: #11D183;
  line-height: 48px;
  text-align: center;
  border-radius: 26px;
  box-shadow: 0px 2px 6px 0px rgba(43, 150, 139, 0.67);
}
.content-box .nav {
  width: 100%;
  background: #fff;
}
.content-box .nav  .header_title_center {
  left: 240px !important;
}
.content-box .nav  .header_title_center > image {
  width: 24px;
  height: 24px;
}
.content-box .nav  .header_title_center > image:first-child {
  margin-right: 14px;
}
.content-box .content-area {
  width: 100%;
  flex: 1;
  display: flex;
  flex-direction: column;
}
.content-box .content-area .content-top-area {
  width: 100%;
  display: flex;
  padding: 16px;
  box-sizing: border-box;
  flex-direction: column;
}
.content-box .content-area .content-top-area .top-title {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.content-box .content-area .content-top-area .top-title > image {
  width: 210px;
  height: 137px;
  margin-bottom: 5px;
}
.content-box .content-area .content-top-area .top-title .title-text {
  width: 100%;
  text-align: center;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.content-box .content-area .content-top-area .top-title .title-text > text {
  font-size: 20px;
  color: #101010;
}
.content-box .content-area .content-top-area .top-title .title-text > text:last-child {
  width: 100px;
  margin-left: 6px;
  font-size: 14px;
  margin-top: 6px;
}
.content-box .content-area .center-content-area {
  background: #f5f5f5;
  width: 100%;
  flex: 1;
  padding: 16px;
  box-sizing: border-box;
  margin-top: 10px;
  overflow: auto;
}
.content-box .content-area .center-content-area .alarm-range {
  margin-bottom: 10px;
  width: 100%;
}
.content-box .content-area .center-content-area .alarm-type {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 46px;
  background: #fff;
  padding: 0 6px;
  box-sizing: border-box;
}
.content-box .content-area .center-content-area .alarm-type .alarm-type-left {
  width: 100px;
}
.content-box .content-area .center-content-area .alarm-type .alarm-type-left > text {
  font-size: 16px;
  color: #101010;
}
.content-box .content-area .center-content-area .alarm-type .alarm-type-right {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  flex: 1;
  width: 0;
}
.content-box .content-area .center-content-area .alarm-type .alarm-type-right > text {
  flex: 1;
  margin-right: 2px;
  padding-left: 6px;
  text-align: right;
  box-sizing: border-box;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-size: 16px;
  color: #888888;
}
.content-box .content-area .center-content-area .alarm-type .alarm-type-right  .u-icon {
  margin-top: 2px;
  width: 20px;
}
.content-box .content-area .bottom-btn-area {
  height: 80px;
  background: #f5f5f5;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content-box .content-area .bottom-btn-area > text {
  font-size: 14px;
  display: inline-block;
  border-radius: 26px;
  width: 60%;
  height: 48px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #BBBBBB;
  color: #fff;
}
.content-box .content-area .bottom-btn-area .btnStyle {
  background: #11D183 !important;
}

