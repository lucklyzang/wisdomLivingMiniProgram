page {
  width: 100%;
  height: 100%;
}
.content-box {
  display: flex;
  min-height: 100vh;
  height: 100%;
  width: 100%;
  flex-direction: column;
  padding-bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.content-box ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.content-box .unbind-device-box  .u-drawer .u-drawer-content .u-mode-center-box {
  padding-bottom: 20px;
  box-sizing: border-box;
}
.content-box .unbind-device-box  .u-drawer .u-drawer-content .u-mode-center-box .u-icon__icon {
  color: #101010 !important;
}
.content-box .unbind-device-box  .u-drawer .u-drawer-content .u-mode-center-box .info-title {
  height: 30px;
  display: flex;
  justify-content: center;
  text-align: center;
  padding-top: 24px;
  box-sizing: border-box;
}
.content-box .unbind-device-box  .u-drawer .u-drawer-content .u-mode-center-box .info-title > image {
  vertical-align: middle;
  width: 24px;
  height: 24px;
  margin-right: 10px;
}
.content-box .unbind-device-box  .u-drawer .u-drawer-content .u-mode-center-box .info-title > text {
  font-size: 16px;
  color: #101010;
  vertical-align: middle;
  margin-top: 2px;
}
.content-box .unbind-device-box  .u-drawer .u-drawer-content .u-mode-center-box .info-content {
  height: 80px;
  font-size: 14px;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #101010;
  padding-top: 20px;
  box-sizing: border-box;
}
.content-box .unbind-device-box  .u-drawer .u-drawer-content .u-mode-center-box .info-content > text {
  height: 20px;
}
.content-box .unbind-device-box  .u-drawer .u-drawer-content .u-mode-center-box .btn-area {
  width: 80%;
  height: 50px;
  display: flex;
  margin: 0 auto;
  justify-content: space-between;
  align-items: center;
}
.content-box .unbind-device-box  .u-drawer .u-drawer-content .u-mode-center-box .btn-area > text {
  font-size: 14px;
  display: inline-block;
  height: 34px;
  width: 42%;
  text-align: center;
  line-height: 34px;
  border-radius: 7px;
}
.content-box .unbind-device-box  .u-drawer .u-drawer-content .u-mode-center-box .btn-area > text:first-child {
  color: #11D183;
  border: 1px solid #11D183;
}
.content-box .unbind-device-box  .u-drawer .u-drawer-content .u-mode-center-box .btn-area > text:last-child {
  color: #fff;
  background: #11D183;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content {
  padding-bottom: 40px;
  box-sizing: border-box;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .top-title-name-area {
  height: 60px;
  width: 100%;
  font-size: 18px;
  color: #A7A7A3;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  border: none;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .top-title-name-area:after {
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  background-color: #BBBBBB;
  -webkit-transform: scaleY(0.5);
          transform: scaleY(0.5);
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .accept-alarm-method-list-wrapper {
  padding-top: 10px;
  box-sizing: border-box;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .accept-alarm-method-list-wrapper .accept-alarm-method-list {
  text-align: center;
  margin-bottom: 16px;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .accept-alarm-method-list-wrapper .accept-alarm-method-list > text {
  font-size: 20px;
  color: #101010;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .accept-alarm-method-list-wrapper .accept-alarm-method-list .textMethodStyle {
  color: #11D183 !important;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .cancel-btn {
  width: 100%;
  display: flex;
  height: 80px;
  align-items: center;
  justify-content: center;
}
.content-box .accept-alarm-method-box  .u-drawer .u-drawer-content .cancel-btn > text {
  width: 80%;
  display: inline-block;
  height: 48px;
  font-size: 18px;
  color: #11D183;
  line-height: 48px;
  text-align: center;
  border-radius: 26px;
  box-shadow: 0px 2px 6px 0px rgba(43, 150, 139, 0.67);
}
.content-box .network-box  .u-drawer .u-drawer-content {
  padding-bottom: 40px;
  box-sizing: border-box;
}
.content-box .network-box  .u-drawer .u-drawer-content .accept-alarm-method-list-wrapper {
  padding: 0 0 30px 0;
  box-sizing: border-box;
}
.content-box .network-box  .u-drawer .u-drawer-content .accept-alarm-method-list-wrapper .accept-alarm-method-list {
  margin-bottom: 0;
  display: flex;
  height: 60px;
  padding: 0 10px;
  box-sizing: border-box;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px dashed #BBBBBB;
}
.content-box .network-box  .u-drawer .u-drawer-content .accept-alarm-method-list-wrapper .accept-alarm-method-list > text:last-child {
  flex: 1;
  font-size: 20px;
  color: #898C8C;
  text-align: right;
  padding-left: 10px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.content-box .nav {
  width: 100%;
  background: #fff;
}
.content-box .content-area {
  width: 100%;
  flex: 1;
  background: #F5F5F5;
  padding: 10px;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}
.content-box .content-area .set-list-area {
  flex: 1;
  overflow: auto;
}
.content-box .content-area .set-list-area .set-list {
  padding: 0 6px 0 10px;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 40px;
  background: #fff;
  margin-bottom: 10px;
}
.content-box .content-area .set-list-area .set-list .set-list-left {
  font-size: 14px;
  color: #101010;
}
.content-box .content-area .set-list-area .set-list .set-list-left > text:nth-child(2) {
  font-size: 12px;
  color: #888888;
  margin-left: 4px;
  vertical-align: bottom;
}
.content-box .content-area .set-list-area .set-list .set-list-right {
  flex: 1;
  padding-left: 10px;
  box-sizing: border-box;
  text-align: right;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.content-box .content-area .set-list-area .edit-device-name .set-list-right {
  flex: none !important;
  width: 70% !important;
}
.content-box .content-area .set-list-area .edit-device-name .set-list-right  .u-form .u-form-item {
  border: 1px solid #BBBBBB;
  border-radius: 4px;
  padding: 2px 8px !important;
}
.content-box .content-area .set-list-area .device-room .set-list-right {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.content-box .content-area .set-list-area .device-room .set-list-right > text {
  margin-right: 2px;
}
.content-box .content-area .bottom-btn {
  height: 80px;
  background: #f5f5f5;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content-box .content-area .bottom-btn > text {
  font-size: 14px;
  display: inline-block;
  border-radius: 26px;
  width: 60%;
  height: 48px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #11D183;
  color: #fff;
}

