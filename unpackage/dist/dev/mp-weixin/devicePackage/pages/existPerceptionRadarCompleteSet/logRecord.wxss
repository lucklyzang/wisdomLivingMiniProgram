page {
  width: 100%;
  height: 100%;
}
.content-box {
  display: flex;
  min-height: 100vh;
  height: 100%;
  width: 100%;
  flex-direction: column;
  padding-bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.content-box ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.content-box .nav {
  width: 100%;
  background: #fff;
}
.content-box .nav  .header_title_center {
  left: 210px !important;
  -webkit-transform: translateX(-55%) !important;
          transform: translateX(-55%) !important;
}
.content-box .nav  .header_title_center .u-icon {
  margin-left: 14px;
}
.content-box .content-area {
  width: 100%;
  flex: 1;
  background: #F5F5F5;
  padding: 10px;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  padding: 10px;
  box-sizing: border;
}
.content-box .content-area .log-list-wrapper {
  height: 100%;
  overflow: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}
.content-box .content-area .log-list-wrapper .scroll-view {
  height: 100%;
}
.content-box .content-area .log-list-wrapper  .u-empty {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.content-box .content-area .log-list-wrapper .log-list {
  margin-bottom: 10px;
  display: flex;
}
.content-box .content-area .log-list-wrapper .log-list .log-left > text {
  font-size: 14px;
  color: #101010;
}
.content-box .content-area .log-list-wrapper .log-list .log-left > text:nth-child(2) {
  margin: 0 4px;
}
.content-box .content-area .log-list-wrapper .log-list .log-right > text {
  font-size: 14px;
  color: #101010;
}

