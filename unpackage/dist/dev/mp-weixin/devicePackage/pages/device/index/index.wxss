page {
  width: 100%;
  height: 100vh;
}
.content-box {
  display: flex;
  min-height: 100vh;
  height: 100%;
  width: 100%;
  flex-direction: column;
  background: #f5f5f5;
  padding-bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.content-box ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.content-box .nav {
  width: 100%;
  background: #fff;
}
.content-box .message-title-area {
  height: 40px;
  padding: 0 16px;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
}
.content-box .message-title-area .message-atile-area-left > text {
  font-size: 14px;
}
.content-box .message-title-area .message-atile-area-left > text:first-child {
  color: #E86F50;
  margin-right: 10px;
}
.content-box .message-title-area .message-atile-area-left > text:last-child {
  color: #11D183;
}
.content-box .message-title-area .message-atile-area-right {
  width: 100px;
}
.content-box .message-title-area .message-atile-area-right  .show-box {
  border: none !important;
  color: #fff !important;
  height: 25px !important;
  background: #11D183 !important;
  height: 25px !important;
}
.content-box .message-title-area .message-atile-area-right  .show-box .input {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  display: inline-block;
  line-height: 25px;
  height: 25px;
  padding-right: 4px;
  box-sizing: border-box;
  text-align: center !important;
}
.content-box .message-title-area .message-atile-area-right  .show-box .list {
  color: #101010;
}
.content-box .message-title-area .message-atile-area-right  .show-box .list-container {
  width: 150px !important;
  top: 0;
}
.content-box .message-title-area .message-atile-area-right  .show-box .list-container .popper__arrow {
  display: none !important;
}
.content-box .message-title-area .message-atile-area-right  .show-box .placeholder {
  color: #fff !important;
}
.content-box .message-title-area .message-atile-area-right  .show-box .right-arrow {
  padding-right: 4px;
  box-sizing: border-box;
  color: #fff !important;
}
.content-box .message-content-area {
  flex: 1;
  overflow: auto;
  display: flex;
  flex-direction: column;
  padding: 0 16px 16px 16px;
  box-sizing: border-box;
}
.content-box .message-content-area .message-list {
  text-align: center;
  margin-top: 15px;
}
.content-box .message-content-area .message-list > text {
  display: inline-block;
  height: 25px;
  font-size: 12px;
  width: 125px;
  background: #BBBBBB;
  text-align: center;
  border-radius: 15px;
  color: #fff;
  line-height: 25px;
}
.content-box .message-content-area .message-list .message-inner-list {
  margin-top: 15px;
  padding: 0 10px 10px 10px;
  box-sizing: border-box;
  box-shadow: 0px 2px 6px 0 rgba(92, 133, 136, 0.29);
  border-radius: 8px;
}
.content-box .message-content-area .message-list .message-inner-list .message-inner-list-top {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 46px;
  position: relative;
  border: none;
}
.content-box .message-content-area .message-list .message-inner-list .message-inner-list-top:after {
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  background-color: #BBBBBB;
  -webkit-transform: scaleY(0.5);
          transform: scaleY(0.5);
}
.content-box .message-content-area .message-list .message-inner-list .message-inner-list-top .inner-list-left > image {
  width: 24px;
  height: 24px;
  margin-right: 10px;
  vertical-align: middle;
}
.content-box .message-content-area .message-list .message-inner-list .message-inner-list-top .inner-list-left > text {
  color: #101010;
  font-size: 16px;
  vertical-align: middle;
}
.content-box .message-content-area .message-list .message-inner-list .message-inner-list-top .inner-list-right {
  flex: 1;
  padding-left: 6px;
  box-sizing: border-box;
  text-align: right;
  display: inline-block;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.content-box .message-content-area .message-list .message-inner-list .message-inner-list-top .inner-list-right > text {
  color: #737373;
  margin-right: 14px;
  font-size: 12px;
}
.content-box .message-content-area .message-list .message-inner-list .message-inner-list-top .inner-list-right > text:nth-child(1) {
  position: relative;
}
.content-box .message-content-area .message-list .message-inner-list .message-inner-list-top .inner-list-right > text:nth-child(1) ::after {
  content: "";
  position: absolute;
  top: 1px;
  right: -8px;
  width: 1px;
  height: 14px;
  background: #BBBBBB;
}
.content-box .message-content-area .message-list .message-inner-list .message-inner-list-top .inner-list-right > text:nth-child(2) {
  position: relative;
}
.content-box .message-content-area .message-list .message-inner-list .message-inner-list-top .inner-list-right > text:nth-child(2) ::after {
  content: "";
  position: absolute;
  top: 1px;
  right: -8px;
  width: 1px;
  height: 14px;
  background: #BBBBBB;
}
.content-box .message-content-area .message-list .message-inner-list .message-inner-list-top .inner-list-right > text:last-child {
  margin-right: 0 !important;
}
.content-box .message-content-area .message-list .message-inner-list .message-inner-list-bottom {
  color: #888888;
  font-size: 14px;
  margin-top: 10px;
  text-align: justify;
}

