page {
  width: 100%;
  height: 100%;
}
.content-box {
  display: flex;
  min-height: 100vh;
  height: 100%;
  width: 100%;
  flex-direction: column;
  padding-bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.content-box ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.content-box .nav {
  width: 100%;
  background: #fff;
}
.content-box .nav  .header_title_center {
  left: 210px !important;
  -webkit-transform: translateX(-55%) !important;
          transform: translateX(-55%) !important;
}
.content-box .nav  .header_title_center .u-icon {
  margin-left: 14px;
}
.content-box .content-area {
  width: 100%;
  flex: 1;
  background: #F5F5F5;
  display: flex;
  flex-direction: column;
}
.content-box .content-area .top-title {
  padding: 10px;
  box-sizing: border-box;
  height: 35vh;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.content-box .content-area .top-title > image {
  width: 100%;
}
.content-box .content-area .top-title > text {
  font-size: 14px;
  color: #101010;
}
.content-box .content-area .bottom-area {
  flex: 1;
  height: 0;
  display: flex;
  flex-direction: column;
  background: #fff;
}
.content-box .content-area .bottom-area .statistics-area {
  height: 60px;
  padding: 0 20px;
  box-sizing: border-box;
}
.content-box .content-area .bottom-area .statistics-area .statistics-content {
  margin-top: -50px;
  padding: 0 20px;
  box-sizing: border-box;
  width: 100%;
  height: 90px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  border-radius: 12px;
  box-shadow: 0px 2px 8px -1px #c7dadb;
}
.content-box .content-area .bottom-area .statistics-area .statistics-content .statistics-content-left > text {
  font-size: 14px;
  color: #101010;
}
.content-box .content-area .bottom-area .statistics-area .statistics-content .statistics-content-left > text:nth-child(2) {
  font-size: 28px;
  color: #289E8E;
  margin: 0 8px 0 20px;
}
.content-box .content-area .bottom-area .statistics-area .statistics-content .statistics-content-right > text {
  font-size: 14px;
  color: #101010;
}
.content-box .content-area .bottom-area .statistics-area .statistics-content .statistics-content-right > text:nth-child(2) {
  font-size: 28px;
  color: #3B9DF9;
  margin: 0 8px 0 20px;
}
.content-box .content-area .bottom-area .log-list-wrapper {
  flex: 1;
  border-top: 1px dashed #BBBBBB;
  padding-top: 10px;
  box-sizing: border-box;
  overflow: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}
.content-box .content-area .bottom-area .log-list-wrapper .scroll-view {
  height: 100%;
}
.content-box .content-area .bottom-area .log-list-wrapper  .u-empty {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.content-box .content-area .bottom-area .log-list-wrapper .log-list {
  padding: 0 10px;
  text-align: center;
  box-sizing: border-box;
  margin-bottom: 10px;
  display: flex;
  justify-content: center;
}
.content-box .content-area .bottom-area .log-list-wrapper .log-list .log-list-left {
  margin-right: 4px;
}
.content-box .content-area .bottom-area .log-list-wrapper .log-list .log-list-left > text {
  font-size: 14px;
  color: #101010;
}
.content-box .content-area .bottom-area .log-list-wrapper .log-list .log-list-left > text:nth-child(1) {
  margin-right: 4px;
}
.content-box .content-area .bottom-area .log-list-wrapper .log-list .log-list-right .textStyleOne {
  color: #b90019 !important;
}
.content-box .content-area .bottom-area .log-list-wrapper .log-list .log-list-right .textStyleTwo {
  color: #E86F50 !important;
}
.content-box .content-area .bottom-area .log-list-wrapper .log-list .log-list-right > text {
  font-size: 14px;
  color: #101010;
}
.content-box .content-area .bottom-area .log-list-wrapper .log-list .log-list-right > text:nth-child(2) {
  margin: 0 4px;
  color: #289E8E;
}
.content-box .content-area .bottom-area .log-list-wrapper .log-list .log-list-right > text:nth-child(4) {
  color: #3B9DF9;
  margin-left: 4px;
}

