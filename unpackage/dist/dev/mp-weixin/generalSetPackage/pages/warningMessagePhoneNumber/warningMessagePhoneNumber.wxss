page {
  width: 100%;
  height: 100%;
}
.content-box {
  display: flex;
  min-height: 100vh;
  height: 100%;
  width: 100%;
  flex-direction: column;
  padding-bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.content-box ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.content-box .nav {
  width: 100%;
  background: #fff;
}
.content-box .content-area {
  width: 100%;
  flex: 1;
  background: #F5F5F5;
  padding: 10px;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}
.content-box .content-area .family-name-area {
  margin-bottom: 10px;
}
.content-box .content-area .family-name-area .dropdown-area {
  width: 100px;
}
.content-box .content-area .family-name-area .dropdown-area  .show-box {
  border: none !important;
  font-size: 16px;
  padding: 0 !important;
  height: 40px;
  color: #0E2442 !important;
  background: transparent !important;
}
.content-box .content-area .family-name-area .dropdown-area  .show-box .input {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  display: inline-block;
  line-height: 40px;
  padding-right: 4px;
  box-sizing: border-box;
}
.content-box .content-area .family-name-area .dropdown-area  .show-box .list {
  color: #101010;
}
.content-box .content-area .family-name-area .dropdown-area  .show-box .list .item.active {
  color: #11D183 !important;
}
.content-box .content-area .family-name-area .dropdown-area  .show-box .list-container {
  width: 150px !important;
  top: -10px;
}
.content-box .content-area .family-name-area .dropdown-area  .show-box .list-container .popper__arrow {
  display: none !important;
}
.content-box .content-area .family-name-area .dropdown-area  .show-box .placeholder {
  color: #0E2442 !important;
}
.content-box .content-area .family-name-area .dropdown-area  .show-box .right-arrow {
  color: #0E2442 !important;
  right: -10px !important;
}
.content-box .content-area .phone-number-list-area {
  width: 100%;
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  overflow: auto;
  position: relative;
}
.content-box .content-area .phone-number-list-area  .u-empty {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.content-box .content-area .phone-number-list-area .family-list {
  width: 100%;
  margin-bottom: 10px;
}
.content-box .content-area .phone-number-list-area .family-list .list-serial {
  font-size: 14px;
  color: #101010;
  margin-bottom: 10px;
}
.content-box .content-area .phone-number-list-area .family-list .list-content {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 10px;
  box-sizing: border-box;
  height: 53px;
  background: #fff;
  border-radius: 10px;
  box-shadow: 0px 2px 6px 0px rgba(92, 133, 136, 0.26);
}
.content-box .content-area .phone-number-list-area .family-list .list-content .list-left {
  font-size: 14px;
  color: #101010;
  vertical-align: middle;
}
.content-box .content-area .phone-number-list-area .family-list .list-content .list-right > image {
  width: 24px;
  height: 24px;
  vertical-align: middle;
}
.content-box .content-area .phone-number-list-area .family-list .list-content .list-right > image:first-child {
  margin-right: 16px;
}
.content-box .content-area .content-bottom-area {
  width: 100%;
  height: 100px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.content-box .content-area .content-bottom-area .content-bottom {
  width: 100%;
  height: 53px;
  border: 1px solid #11D183;
  border-radius: 29px;
  display: flex;
  align-items: center;
  justify-content: center;
}

