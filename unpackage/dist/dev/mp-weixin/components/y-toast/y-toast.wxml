<view class="y-toast data-v-252151ac"><block wx:if="{{isShow}}"><view data-event-opts="{{[['touchmove',[['moveHandle',['$event']]]]]}}" class="y-toast-overlay data-v-252151ac" catchtouchmove="__e"><block wx:if="{{tmpConfig.icon}}"><view class="y-toast__content data-v-252151ac"><block wx:if="{{tmpConfig.type==='loading'}}"><view class="loading data-v-252151ac"><image src="{{passedPng}}" class="_img data-v-252151ac"></image></view></block><block wx:else><block wx:if="{{tmpConfig.type==='success'}}"><view class="toast-icon data-v-252151ac"><image src="{{passedPng}}" class="_img data-v-252151ac"></image></view></block><block wx:else><block wx:if="{{tmpConfig.type==='warning'}}"><view class="toast-icon data-v-252151ac"><image src="{{exclamationPointPng}}" class="_img data-v-252151ac"></image></view></block><block wx:else><view class="toast-icon data-v-252151ac"><image src="{{exclamationPointPng}}" class="_img data-v-252151ac"></image></view></block></block></block><text class="y-toast__content__text data-v-252151ac">{{''+tmpConfig.message+''}}</text></view></block><block wx:if="{{!tmpConfig.icon}}"><view class="toastText data-v-252151ac" style="{{(Tstyle)}}"><text class="y-toast__content__text data-v-252151ac">{{''+tmpConfig.message+''}}</text></view></block></view></block></view>